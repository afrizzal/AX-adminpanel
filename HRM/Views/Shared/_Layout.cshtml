            $(".numberonly").keydown(function (e) {
                // Allow: backspace, delete, tab, escape, enter and .
                if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                    // Allow: Ctrl/cmd+A
                    (e.keyCode == 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: Ctrl/cmd+V
                    (e.keyCode == 86 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: Ctrl/cmd+C
                    (e.keyCode == 67 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: Ctrl/cmd+X
                    (e.keyCode == 88 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: home, end, left, right
                    (e.keyCode >= 35 && e.keyCode <= 39)) {
                    // let it happen, don't do anything
                    return;
                }
                // Ensure that it is a number and stop the keypress
                if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                    e.preventDefault();
                }
            });
            $(".numberonly").keyup(function (e) {
                // Clear String after paste
                if ((e.keyCode == 86 || (e.ctrlKey === true || e.metaKey === true))) {
                    var $this = $(this)
                    if ($this.val() != $this.val().replace(/[^\d\.]/g, "")) {
                        $this.val($this.val().replace(/[^\d\.]/g, ""));
                    }
                }
            });

            $(".allownumericwithdecimal").on("keypress keyup blur", function (event) {
                //this.value = this.value.replace(/[^0-9\.]/g,'');
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
                if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                }
            });

            $(".allownumericwithoutdecimal").on("keypress keyup blur", function (event) {
                $(this).val($(this).val().replace(/[^\d].+/, ""));
                if ((event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                }
            });

            $(".allownumericwithNegative").on("keypress keyup blur", function (event) {
                $(this).val($(this).val().replace(/[^0-9\.-]/g, ''));
                if ((event.which != 45 || $(this).val().indexOf('-') != -1) && (event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                }
            });

            var OpenAuthorizationPopup = @Html.Raw(Json.Encode(this.ViewData["OpenAuthorizationPopup"]));
            if (OpenAuthorizationPopup == "Open")
                $('#modal-login').modal('show');
        });

        function SetDataToTableNotification(tablename, colgroup, data) {
            var table = $(tablename).DataTable();
            table.destroy();
            $(tablename).empty();

            var coldate = [];
            var colnum = [];
            var cols = [];
            var colidx = 0;
            $.each(data.tblcols, function (key, val) {
                if (val.indexOf("num_") != -1) {
                    colnum.push(colidx);
                    val = val.replace("num_", "");
                }
                var obj = { title: val };
                cols.push(obj);
                if (val.toUpperCase().indexOf("DATE") != -1)
                    coldate.push(colidx);
                colidx++;
            });

            var rows = [];
            var total = 0;
            $.each(data.tblrows, function (key, val) {
                var row = Object.values(val);
                rows.push(row);
                total++;
            });

            if (total > 0) {
                if (colgroup != null) {
                    table = $(tablename).DataTable({
                        "columns": cols,
                        "data": rows,
                        "deferRender": true,
                        "bInfo": false,
                        "lengthMenu": [ 5, 10, 25, 50, 75, 100 ],
                        "columnDefs": [
                        {
                            "visible": false,
                            "targets": colgroup
                        },
                        {
                            "type": "date",
                            "targets": coldate
                        },
                        {
                            "className": "text-right",
                            "targets": colnum
                        }],
                        "drawCallback": function (settings) {
                            var api = this.api();
                            var rows = api.rows({ page: 'current' }).nodes();
                            var last = null;
                            api.column(colgroup, { page: 'current' }).data().each(function (group, i) {
                                if (last !== group) {
                                    $(rows).eq(i).before(
                                        '<tr class="group"><td colspan="' + (colidx + 1) + '">' + group + '</td></tr>'
                                    );
                                    last = group;
                                }
                            });
                        }
                    });
                    $(tablename + ' tbody').on('click', 'tr.group', function () {
                        var currentOrder = table.order()[0];
                        if (currentOrder[0] === colgroup && currentOrder[1] === 'asc') {
                            table.order([colgroup, 'desc']).draw();
                        }
                        else {
                            table.order([colgroup, 'asc']).draw();
                        }
                    });
                }
                else {
                    table = $(tablename).DataTable({
                        "columns": cols,
                        "data": rows,
                        "deferRender": true,
                        "bInfo": false,
                        "lengthMenu": [5, 10, 25, 50, 75, 100],
                        "columnDefs": [
                        {
                            "type": "date",
                            "targets": coldate
                        },
                        {
                            "className": "text-right",
                            "targets": colnum
                        }]
                    });
                }
            }
            return total;
        }

