        <script>
        var nIntervId;
        var d = new Date("@(ServerTime.ToString())");

        function startTimer() {
            nIntervId = setInterval(setTimer, 1000);
        }

        function setTimer() {
            d = moment(d).add(1, 's').toDate();
            $("#servertime").html(moment(d).format('DD MMM YYYY HH:mm:ss'))
        }

        function stopTimer() {
            clearInterval(nIntervId);
        }

        function getNotification() {
            var notiftotal = $("#notiftotal");
            var notifheader = $("#notifheader");
            var notiflist = $("#notiflist");
            var UserID = "@(UserID)";
            notiftotal.removeClass("label-warning");
            notiftotal.html('<i class="fa fa-refresh fa-spin"></i>');
            $.ajax({
                url: "@(Url.Action("GetNotification", "Home"))",
                data: { UserID: UserID },
                cache: false,
                type: "POST",
                success: function (data) {
                    var listnotif = "";
                    var total = 0;
                    $.each(data.dtnotifapp, function (key, val) {
                        listnotif += '<li><a href="' + val.notiflink + '"><i class="' + val.notifcss + '"></i> ' + val.notifmsg + '</a></li>';
                        total += val.notifqty;
                    });
                    notiftotal.html(total);
                    if (total == 0) {
                        notiftotal.hide();
                    } else {
                        notiftotal.show();
                    }
                    notifheader.html(total + " Notification");
                    notiflist.html(listnotif);
                    if ($("#divnotiflistapp").length > 0) {
                        SetNotification(data);
                    }
                },
                error: function (xhr, status, p3, p4) {
                    console.dir(xhr);
                }
            });
        }

        $(document).ready(function () {
            startTimer();
            getNotification();
            $('.datatable').DataTable();
        });
        </script>

